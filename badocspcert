#!/bin/bash

if [ -z "$1" ]; then
	echo -e "Usage:"
	echo -e "\t$0 [host]"
	exit 1
fi

TMP="$(mktemp -d)"

openssl s_client -connect "$1:443" -showcerts < /dev/null &> "$TMP/_ossl.txt"

csplit -s -f "$TMP/_pem" "$TMP/_ossl.txt" '/-----BEGIN CERTIFICATE-----/' '{*}'

for pem in "$TMP"/_pem*; do
	hash=$(openssl x509 -in "$pem" -outform der 2>/dev/null | \
		openssl dgst -sha256 -binary | base64)
	grep -q "$hash" "$0"; then
                affected=true
        fi
done

if $affected; then
        echo "$1 IS affected";
else
        echo "$1 NOT affected";
fi 

rm -r "$TMP"

# We grep this script for the affected intermediates as base64 encoded sha256 hashes,
# so we list them here as a comment:
